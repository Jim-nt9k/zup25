Перем СписокСтандартныхПараметров Экспорт;

Функция ОткрытьWord(стПараметры, Видимость = Истина) Экспорт
	WordApp = стПараметры.WordApp;
	WordDoc = стПараметры.WordDoc;
	ПутьКФайлу = стПараметры.ПутьКФайлу;
	
	Попытка
		WordApp = Новый COMОбъект("Word.Application");
	Исключение
		Сообщить("Компонента WordApp не установлена на данном компьютере!", СтатусСообщения.Важное);
	КонецПопытки;
	
	
	Если Видимость Тогда
		WordApp.Visible = 1;
	Иначе
		WordApp.Visible = 0;
	КонецЕсли;
	
	Если Прав(ПутьКФайлу, 3) = "dot" Тогда
		WordDoc = WordApp.Documents.Add(ПутьКФайлу);
	ИначеЕсли Прав(ПутьКФайлу, 3) = "doc" Тогда
		WordDoc = WordApp.Documents.Open(ПутьКФайлу);
	Иначе
		Сообщить("не верное расшерение файла");
		Возврат Неопределено;
	КонецЕсли;
	
	стПараметры.WordApp = WordApp;
	стПараметры.WordDoc = WordDoc;
	
	Возврат WordApp;
	
КонецФункции        

Функция вИнициализацияМенеджеров() Экспорт
	Менеджеры = Новый Соответствие;
	
	ИмяТипа = "Справочник";
	Для каждого ОбъектМД Из Метаданные.Справочники Цикл
		Имя = ОбъектМД.Имя;
		Менеджер = Справочники[Имя];
		ТипСсылкиСтрокой = "СправочникСсылка." + Имя;
		ТипСсылки = Тип(ТипСсылкиСтрокой);
		Структура = Новый Структура("Имя,ИмяТипа,ТипСсылкиСтрокой,Менеджер,ТипСсылки, ОбъектМД", Имя, ИмяТипа, ТипСсылкиСтрокой, Менеджер, ТипСсылки, ОбъектМД);
		Менеджеры.Вставить(ОбъектМД, Структура);
	КонецЦикла;
	
	ИмяТипа = "Документ";
	Для каждого ОбъектМД Из Метаданные.Документы Цикл
		Имя = ОбъектМД.Имя;
		Менеджер = Документы[Имя];
		ТипСсылкиСтрокой = "ДокументСсылка." + Имя;
		ТипСсылки = Тип(ТипСсылкиСтрокой);
		Структура = Новый Структура("Имя,ИмяТипа,ТипСсылкиСтрокой,Менеджер,ТипСсылки, ОбъектМД", Имя, ИмяТипа, ТипСсылкиСтрокой, Менеджер, ТипСсылки, ОбъектМД);
		Менеджеры.Вставить(ОбъектМД, Структура);
	КонецЦикла;
	
	Возврат Менеджеры;
	
КонецФункции

