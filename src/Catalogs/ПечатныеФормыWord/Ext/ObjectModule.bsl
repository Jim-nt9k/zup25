		Возврат ПолучитьИзСтандартногоСписка(СтандартныйНабор);
	Иначе
		Если Не ЗначениеЗаполнено(ОбъектВладелец) Тогда
			Возврат "";
		КонецЕсли;
		
		Попытка
			Значение = Неопределено;
			Выполнить("Значение = ОбъектВладелец." + ПутьКДанным);
			Возврат Строка(Значение);
		Исключение
			Сообщить("Не обнаружени данные по :" + ОбъектВладелец + "." + ПутьКДанным, СтатусСообщения.Важное);
			Возврат "";
		КонецПопытки;
	КонецЕсли;
