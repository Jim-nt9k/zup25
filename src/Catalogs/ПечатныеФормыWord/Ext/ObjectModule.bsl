Функция ПолучитьИзСтандартногоСписка(СтандартныйНабор) Экспорт
	Если Не ЗначениеЗаполнено(ОбъектВладелец) Тогда
		Возврат "";
	КонецЕсли;
	Результат = "";
	
	
	Попытка
		Если СтандартныйНабор = "Реквизит..." Тогда
			Результат = "";
		ИначеЕсли СтандартныйНабор = "<Нет>" Тогда
			Результат = "";
			
			
		ИначеЕсли СтандартныйНабор = "Счет организации" Тогда
			
			Результат = СокрЛП(?(ЗначениеЗаполнено(ОбъектВладелец.СчетОрганизации.НомерСчета), ОбъектВладелец.СчетОрганизации.НомерСчета, ОбъектВладелец.СчетОрганизации.IBAN));
			
		ИначеЕсли СтандартныйНабор = "Организация наименование англискои" Тогда
			
			Результат = СокрЛП(ОбъектВладелец.Организация.НаименованиеАНГЛ);
			
		ИначеЕсли СтандартныйНабор = "Сумма целое" Тогда
			
			Результат = Цел(ОбъектВладелец.СуммаДокумента);
			
		ИначеЕсли СтандартныйНабор = "Сумма дробь" Тогда
			
			Дробь = Строка(ОбъектВладелец.СуммаДокумента - Цел(ОбъектВладелец.СуммаДокумента));
			Пока Цел(Дробь) <> Дробь Цикл
				Дробь = Дробь * 10;
			КонецЦикла;
			
			Результат = Дробь;
			
			
		ИначеЕсли СтандартныйНабор = "Код валюты" Тогда
			
			Результат = ?(Строка(ОбъектВладелец.ВалютаДокумента.Наименование) = "грн", "UAH", Строка(ОбъектВладелец.ВалютаДокумента.Наименование));
			
		ИначеЕсли СтандартныйНабор = "Назначение платежа" Тогда
			
			Результат = СокрЛП(СтрЗаменить(ОбъектВладелец.НазначениеПлатежа, Символы.ПС, " "));
			
			
		ИначеЕсли СтандартныйНабор = "Дата день" Тогда
			
			Результат = ?(ЗначениеЗаполнено(ОбъектВладелец.Дата), Формат(ОбъектВладелец.Дата, "ДФ=dd"), Формат(ОбъектВладелец.ДатаВыписки, "ДФ=dd"));
			
		ИначеЕсли СтандартныйНабор = "Дата месяць" Тогда
			
			Результат = ?(ЗначениеЗаполнено(ОбъектВладелец.Дата), Формат(ОбъектВладелец.Дата, "ДФ=MM"), Формат(ОбъектВладелец.ДатаВыписки, "ДФ=MM"));
			
		ИначеЕсли СтандартныйНабор = "Дата год" Тогда
			
			Результат = ?(ЗначениеЗаполнено(ОбъектВладелец.Дата), Формат(ОбъектВладелец.Дата, "ДФ=yyyy"), Формат(ОбъектВладелец.ДатаВыписки, "ДФ=yyyy"));
			
		ИначеЕсли СтандартныйНабор = "Дата выписки день" Тогда
			
			Результат = Формат(ОбъектВладелец.ДатаВыписки, "ДФ=dd");
			
		ИначеЕсли СтандартныйНабор = "Дата выписки месяць" Тогда
			
			Результат = Формат(ОбъектВладелец.ДатаВыписки, "ДФ=MM");
			
		ИначеЕсли СтандартныйНабор = "Дата выписки год" Тогда
			
			Результат = Формат(ОбъектВладелец.ДатаВыписки, "ДФ=yyyy");
			
		ИначеЕсли СтандартныйНабор = "Банк получателя" Тогда
			
			Результат = СокрЛП(ОбъектВладелец.СчетКонтрагента.Банк.Наименование);
			
		ИначеЕсли СтандартныйНабор = "Банк получателя SWIFT" Тогда
			
			Результат = СокрЛП(ОбъектВладелец.СчетКонтрагента.Банк.SWIFT);
			
			
		КонецЕсли;
	Исключение
		Результат = "";
		Сообщить("Неверно задан путь к данным");
	КонецПопытки;
	
	
	Возврат Результат;
КонецФункции

