Функция ПолучитьПараметы() Экспорт
	Перем ИмяФайла;
	ИмяФайла = ПолучитьИмяВременногоФайла("doc");
	ПараметрыТЧ = Новый ТаблицаЗначений;
	ПараметрыТЧ.Колонки.Добавить("ИмяПараметра");
	РаботаСФайлами.СохранитьФайлНаДиске(МакетWord, ИмяФайла, Ложь, "ДА", Ложь);
	стПараметры = Новый Структура("ПутьКФайлу,WordApp,WordDoc", ИмяФайла);
	WordApp = ОткрытьWord(стПараметры, Ложь);
	Документ = WordApp.ActiveDocument();
	КолВоВТелеДока = Документ.Fields.count();
	
	Документ.ActiveWindow.ActivePane.View.SeekView = 1;
	Документ.ActiveWindow.ActivePane.Selection.WholeStory();
	ПараметрыКолонтитулов = Документ.ActiveWindow.ActivePane.Selection.Fields;
	КолВоВКолунтитулах = ПараметрыКолонтитулов.count();
	
	КолВо = КолВоВТелеДока + КолВоВКолунтитулах;
	
	Для инд = 1 По КолВоВКолунтитулах Цикл
		Если ПараметрыКолонтитулов.item(инд).Type <> 17 Тогда
			Продолжить;
		КонецЕсли;
		
		ИмяПараметра = ПараметрыКолонтитулов.item(инд).Result.Text;
		строка = ПараметрыТЧ.Найти(ИмяПараметра, "ИмяПараметра");
		Если строка = Неопределено Тогда
			НовСтр = ПараметрыТЧ.Добавить();
			НовСтр.ИмяПараметра = ИмяПараметра;
			НовСтр.Значение = "";
		КонецЕсли;
	КонецЦикла;
	Документ.ActiveWindow.ActivePane.View.SeekView = 0;
	Для инд = 1 По КолВоВТелеДока Цикл
		Если Документ.Fields.item(инд).Type <> 17 Тогда
			Продолжить;
		КонецЕсли;
		ИмяПараметра = Документ.Fields.item(инд).Result.Text;
		строка = ПараметрыТЧ.Найти(ИмяПараметра, "ИмяПараметра");
		Если строка = Неопределено Тогда
			НовСтр = ПараметрыТЧ.Добавить();
			НовСтр.ИмяПараметра = ИмяПараметра;
		КонецЕсли;
	КонецЦикла;
	WordApp.Quit(0);
	Возврат ПараметрыТЧ;
КонецФункции

Функция ПолучитьИзСтандартныйНабор(СтандартныйНабор, ПутьКДанным)
