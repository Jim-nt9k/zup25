			ФорматнаяСтрока = Формат(Выборка[ВидРезультата - 1], "ДФ=dd.MM.yyyy");
		Иначе ФорматнаяСтрока = Выборка[ВидРезультата - 1];
		КонецЕсли;
		СтрокаВыборки = СтрокаВыборки + ФорматнаяСтрока + Символы.ПС;
